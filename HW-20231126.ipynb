   "source": [
    "#請輸入學生分數(0-100):\n",
    "#0-60, 不及格\n",
    "#60-80, 及格\n",
    "#80-100, 優\n",
    "#檢查超出範圍, 格式\n",
    "\n",
    "import pyinputplus as pyip\n",
    "\n",
    "scores = pyip.inputNum(prompt=\"請輸入學生分數\", min=0, max=100)\n",
    "print(scores)\n",
    "\n",
    "if scores < 60:\n",
    "    print(\"不及格\")\n",
    "elif scores >=60 & scores < 80:\n",
    "    print(\"及格\")\n",
    "else:\n",
    "    print(\"優\")"
   ]
  }
